{
  "name": "fint-informasjonsmodell",
  "version": "1.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "dev": "concurrently --kill-others \"npm run server:build:watch\" \"npm run server:watch\"",
    "clean": "rm -rf dist",
    "start": "npm run server",
    "build": "npm run server:build && npm run client:build",
    "server": "node ./dist/index.js",
    "server:watch": "nodemon ./dist/index.js -w dist -d .2",
    "server:build": "tsc --project ./server/tsconfig.json",
    "server:build:watch": "npm run server:build -- -w",
    "client": "ng serve --proxy-config proxy.conf.json",
    "client:build": "ng build --prod",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.1",
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/material": "~2.0.0-beta.3",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@angular/router": "^4.0.1",
    "chalk": "~1.1.3",
    "core-js": "^2.4.1",
    "d3": "~4.7.3",
    "d3-scale-chromatic": "^1.1.1",
    "d3-selection-multi": "~1.0.0",
    "express": "~4.15.2",
    "fint-shared-components": "github:fintprosjektet/fint-shared-components",
    "font-awesome": "~4.7.0",
    "iconv-lite": "~0.4.15",
    "markdown-to-html-pipe": "~1.2.0",
    "mkdirp": "~0.5.1",
    "morgan": "~1.8.1",
    "request": "~2.81.0",
    "rxjs": "^5.1.0",
    "serve-favicon": "~2.4.1",
    "ts-helpers": "^1.1.1",
    "winston": "~2.3.0",
    "xml2js": "~0.4.17",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0",
    "@angular/compiler-cli": "^4.0.1",
    "@types/chalk": "~0.4.31",
    "@types/d3": "~4.7.0",
    "@types/d3-scale-chromatic": "^1.0.1",
    "@types/d3-selection-multi": "~1.0.3",
    "@types/express": "~4.0.34",
    "@types/iconv-lite": "0.0.1",
    "@types/jasmine": "~2.5.47",
    "@types/morgan": "~1.7.32",
    "@types/node": "~7.0.8",
    "@types/request": "~0.0.41",
    "@types/serve-favicon": "~2.2.28",
    "@types/winston": "~2.3.0",
    "@types/xml2js": "~0.0.33",
    "codelyzer": "~2.1.1",
    "concurrently": "~3.4.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.0.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.11.0",

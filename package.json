{
  "name": "fint-informasjonsmodell",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "dev": "concurrently \"npm run build:server:watch\" \"npm run start:server:watch\"",
    "start": "npm run start:server",
    "start:server": "node ./dist/index.js",
    "start:server:watch": "nodemon ./dist/index.js -w dist -d .2",
    "start:client": "ng serve --proxy-config proxy.conf.json",
    "build": "npm run build:server && npm run build:client",
    "build:client": "ng build",
    "build:server": "tsc --project ./server/tsconfig.json",
    "build:server:watch": "npm run build:server -- -w",
    "clean": "rm -rf dist",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.0",
    "@angular/compiler": "^2.4.0",
    "@angular/core": "^2.4.0",
    "@angular/forms": "^2.4.0",
    "@angular/http": "^2.4.0",
    "@angular/material": "~2.0.0-beta.1",
    "@angular/platform-browser": "^2.4.0",
    "@angular/platform-browser-dynamic": "^2.4.0",
    "@angular/router": "^3.4.0",
    "chalk": "~1.1.3",
    "core-js": "^2.4.1",
    "d3": "~4.3.0",
    "d3-selection-multi": "~1.0.0",
    "express": "~4.14.0",
    "fint-shared-components": "github:fintprosjektet/fint-shared-components",
    "font-awesome": "~4.7.0",
    "iconv-lite": "~0.4.15",
    "lodash": "4.17.2",
    "markdown-to-html-pipe": "~1.2.0",
    "mkdirp": "~0.5.1",
    "morgan": "~1.7.0",
    "request": "~2.79.0",
    "rxjs": "^5.0.1",
    "serve-favicon": "~2.3.2",
    "ts-helpers": "^1.1.1",
    "winston": "~2.3.0",
    "xml2js": "~0.4.17",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-beta.30",
    "@angular/compiler-cli": "^2.4.0",
    "@types/chalk": "~0.4.31",
    "@types/d3": "~4.3.0",
    "@types/d3-selection-multi": "~1.0.3",
    "@types/express": "~4.0.34",
    "@types/iconv-lite": "0.0.1",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "4.14.39",
    "@types/morgan": "~1.7.32",
    "@types/node": "^6.0.42",
    "@types/request": "0.0.36",
    "@types/serve-favicon": "~2.2.28",
    "@types/winston": "~2.2.0",
    "@types/xml2js": "0.0.32",
    "codelyzer": "~2.0.0-beta.1",
    "concurrently": "~3.1.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "~4.0.13",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.0.3"
  }

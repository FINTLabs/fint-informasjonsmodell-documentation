  "scripts": {
    "ng": "ng",
    "dev": "concurrently --kill-others \"npm run server:build:watch\" \"npm run server:watch\"",
    "clean": "rm -rf dist",
    "start": "npm run server",
    "build": "npm run ng --version && npm run server:build && npm run client:build",
    "server": "node ./dist/index.js",
    "server:watch": "npx nodemon ./dist/index.js -w dist -d .2",
    "server:build": "npx tsc --project ./server/tsconfig.json",
    "server:build:watch": "npm run server:build -- -w",
    "client": "ng serve --proxy-config proxy.conf.json",
    "client:build": "ng build --configuration=production",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },

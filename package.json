{
  "name": "fint-informasjonsmodell",
  "version": "1.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "dev": "concurrently --kill-others 'yarn run server:build:watch' 'yarn run server:watch' 'yarn run client:build:watch'",
    "clean": "rm -rf dist node_modules",
 
    "build": "yarn run server:build && yarn run client:build",
    "start": "yarn run server",    
    
    "server": "node ./dist/server/index.js",
    "server:watch": "nodemon --watch dist/server --delay 0.2 --exec node dist/server/index.js",
    "server:build": "tsc --project ./server/tsconfig.json",
    "server:build:watch": "yarn run server:build -- -w",
    
    "client": "ng serve --proxy-config proxy.conf.json",
    "client:build": "ng build --configuration=production",
    "client:build:watch": "ng build --configuration=development --output-path=dist/public --watch",
    
    "test": "ng test",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix"
  },
  "private": true,
  "dependencies": {

{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome against localhost, with sourcemaps",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:4200/*",
      "sourceMaps": true,
      "webRoot": "${workspaceRoot}/client",
      "sourceMapPathOverrides": {
        "webpack:///D:/dev/FINT/fint-informasjonsmodell-documentation/*": "${workspaceRoot}/*"
      }
    },
    {
      "name": "Attach to Chrome, with sourcemaps",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "sourceMaps": true,
      "webRoot": "${workspaceRoot}/client",
      "url": "http://localhost:4200/*",
      "sourceMapPathOverrides": {
        "webpack:///D:/dev/FINT/fint-informasjonsmodell-documentation/*": "${workspaceRoot}/*"
      }
    }
  ]
}
